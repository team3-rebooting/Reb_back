<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="fileAdmin">
	<!-- 파일 추가 -->
	<insert id="insert" parameterType="FileAdminDTO">
		insert into tbl_file_admin 
		values(#{fileSystemName}, #{fileOriginalName}, #{AdminNumber})
	</insert>
	
	<!-- 파일 조회 -->
	<select id="select" parameterType="int" resultType="FileAdminDTO">
		select file_system_name, file_original_name, admin_number
		from tbl_file_admin 
		where admin_number = #{adminNumber}
	</select>
	
	<delete id="delete" parameterType="int">
		delete from tbl_file_admin 
		where admin_number = #{adminNumber}
	</delete>
</mapper>